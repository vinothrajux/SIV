<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:background="@color/bodybg"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <!-- LinearLayout Rows-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <!-- LinearLayout Columns-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="5dp">
                <!-- This is for First Icon Start Row 1 Col 1 -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                        <TextView
                            style="@style/TextViewLabel"
                            android:text="Enter StaffId"
                            />
                        <SearchView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/applicationno"
                            style="@style/SearchBox"
                            >

                        </SearchView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">


                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Staff ID"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/StaffId"
                        />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Name"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Name"
                        />
                    </LinearLayout>

                    <LinearLayout
                          android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Branch Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/BranchCode"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Branch"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Branch"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Designation"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Designation"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Total No Of Experience in Teaching"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Experience"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Theory Subject List"
                        />
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject1 Code"
                        />

                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/subject1"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject2 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Subject2"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject3 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Subject3"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject4 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Subject4"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject5 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Subject5"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Practical Subject List"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject1 Code"
                        />



                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/PracSubject1"
                        />
                    </LinearLayout>


                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject2 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/PractSubject2"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject3 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/PractSubject3"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject4 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/PractSubject4"
                        />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Subject5 Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/PractSubject5"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">



                    <Button
                        android:id="@+id/loginButton2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="close"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="#919978"
                        android:textColor="#fff"
                        android:paddingLeft="35dp"
                        android:paddingRight="35dp"
                        android:onClick="dashboard"
                        />
                     </LinearLayout>
                </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>