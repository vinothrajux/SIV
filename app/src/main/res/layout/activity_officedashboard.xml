<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:padding="10dp"
    android:background="#f1ffc6"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!-- LinearLayout Rows-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <!-- LinearLayout Columns-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="10dp">
                <!-- This is for First Icon Start Row 1 Col 1 -->
                <LinearLayout
                    android:id="@+id/admissionBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/admission"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Admission"
                        />
                </LinearLayout>  <!-- This is for First Icon End Row 1 Col 1 -->


                <!-- This is for Second Icon Start Row 1 Col 2 -->
                <LinearLayout
                    android:id="@+id/studentBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/student"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Student Data"
                        />
                </LinearLayout>  <!-- This is for Second Icon End Row 1 Col 2 -->


                <!-- This is for Third Icon Start Row 1 Col 3 -->
                <LinearLayout
                    android:id="@+id/staffDataBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/staff"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Staff Data"
                        />
                </LinearLayout>  <!-- This is for Third Icon End Row 1 Col 3 -->



                <!-- This is for Fourth Icon Start Row 1 Col 4 -->
                <LinearLayout
                    android:id="@+id/feeCounterBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/accounts"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Fee Counter"
                        />
                </LinearLayout>  <!-- This is for Fourth Icon End Row 1 Col 4 -->

            </LinearLayout>  <!-- This if for End the First Row -->

            <!-- This if for Start the Second Row -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="10dp"
                >
                <!-- This is for Fifth Icon Start Row 2 Col 1 -->
                <LinearLayout
                    android:id="@+id/storeBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/stores"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Stores"
                        />
                </LinearLayout>  <!-- This is for Fifth Icon End Row 2 Col 1 -->

                <!-- This is for Sixth Icon Start Row 2 Col 2 -->
                <LinearLayout
                    android:id="@+id/transportBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/transport"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Transport"
                        />
                </LinearLayout>  <!-- This is for Sixth Icon End Row 2 Col 2 -->

                <!-- This is for Seventh Icon Start Row 2 Col 3 -->

                <LinearLayout
                    android:id="@+id/hostelBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/hostel"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Hostel"
                        />
                </LinearLayout>  <!-- This is for Seventh Icon Start Row 2 Col 4 -->

                <!-- This is for Eighth Icon End Row 2 Col 4 -->
                <LinearLayout
                    android:id="@+id/examBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/exam"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Exam"
                        />
                </LinearLayout>  <!-- This is for Eighth Icon End Row 2 Col 4 -->

            </LinearLayout>  <!-- This is for Second Row End-->

            <!-- This is for Third Row Start-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <!-- This is for Nineth Icon Start Row 3 Col 1 -->
                <LinearLayout
                    android:id="@+id/eGovernanceBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/governance"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="E-Governance"
                        />
                </LinearLayout>  <!-- This is for Nineth Icon End Row 3 Col 1 -->

                <!-- This is for Tenth Icon Start Row 3 Col 2 -->
                <LinearLayout
                    android:id="@+id/eventsBox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/events"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Events"
                        />
                </LinearLayout>  <!-- This is for Tenth Icon End Row 3 Col 2 -->

                <!-- This is for Eleventh Icon Start Row 3 Col 3-->
                <LinearLayout
                    android:id="@+id/circularBox"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="@drawable/border_radius_orange"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:adjustViewBounds="true"
                        android:src="@drawable/circulars"
                        android:layout_marginBottom="10dp"
                        />
                    <TextView
                        style="@style/icon_label"
                        android:text="Circulars"
                        />
                </LinearLayout> <!-- This is for Eleventh Icon End Row 3 Col 3-->

                <!-- This is for Twelth Icon Start Row 3 Col 4-->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginLeft="5dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >

                </LinearLayout>  <!-- This is for Twelth Icon End Row 3 Col 4-->
            </LinearLayout>  <!-- This is for Third Row End-->

        </LinearLayout>  <!-- This is for Main Linear Layout End -->
    </ScrollView>
</RelativeLayout>