<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:background="@color/bodybg"
    >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <!-- LinearLayout Rows-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <!-- LinearLayout Columns-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="5dp">
                <!-- This is for First Icon Start Row 1 Col 1 -->
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:layout_marginRight="5dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">


                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Reg.No"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/RegNo"
                        />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Admission No"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/AdmissionNo"
                        />
                     </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Name"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Name"
                        />

                     </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Semester"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:text="@+id/Semester"
                        />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Branch Code"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/BranchCode"
                        />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Branch"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Branch"
                        />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Academic Year"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/AcademicYear"
                        />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">


                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Existing Route"
                        />
                    <TextView
                        style="@style/TextViewLabel"
                        android:id="@+id/ExistingRoute"
                        />
                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        style="@style/datapanel">

                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Route Name"
                        />

                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/RouteName"
                        />
                    </LinearLayout>

                     <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">
                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Stage"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Stage"
                        />

                    </LinearLayout>


                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Seat No"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/SeatNo"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Fees"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Fees"
                        />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="New Route"
                        />
                    <TextView
                        style="@style/TextViewLabel"
                        android:id="@+id/NewRoute"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                    <TextView
                        style="@style/TextViewLabel"
                        android:text="Route Name"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/RouteName1"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Stage"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Stage1"
                        />
                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">


                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Seat No"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/SeatNo1"
                        />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="Fees"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/Fees1"
                        />
                    </LinearLayout>


                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <TextView
                        style="@style/TextViewLabel"
                        android:text="User"
                        />
                    <TextView
                        style="@style/TextViewContent"
                        android:id="@+id/User"
                        />

                    </LinearLayout>

                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            style="@style/datapanel">

                        <Button
                        android:id="@+id/loginButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Close"
                        android:layout_gravity="center"
                        android:layout_marginTop="10dp"
                        android:background="#919978"
                        android:textColor="#fff"
                        android:paddingLeft="50dp"
                        android:paddingRight="50dp"
                        android:onClick="dashboard"
                        />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</RelativeLayout>